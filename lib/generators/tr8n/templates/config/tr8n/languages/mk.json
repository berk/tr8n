{
  "locale": "mk",
  "english_name": "Macedonian",
  "native_name": "Македонски",
  "google_key": "mk",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": "(= 1 @size)", "description": "contains 1 element"},
        "other": {"description": "contains at least 2 elements"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "other"],
      "rules": {
        "one":    {"rule": "(&& (= 1 (mod 10 @n)) (!= 11 @n))", "description": "{n} mod 10 is 1 and {n} is not 11", "examples": "1, 21, 31, 41, 51, 61..."},
        "other":  {"examples": "0, 2-20, 22-30, 32-40...; 1.2, 2.07..."}
      }
    }
  },
  "language_cases": {
  }
}
